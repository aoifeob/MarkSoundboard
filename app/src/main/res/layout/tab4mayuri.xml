<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.markapp.MainActivity"
    android:background="#FF88D8F2">


    <Button
        android:text="Tuturu"
        android:onClick="mayuri1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:id="@+id/tururu"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Juicy Chicken #1"
        android:onClick="mayuri2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/juicy"
        android:layout_below="@+id/tururu"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="76dp" />

    <Button
        android:text="Chicken Tenders"
        android:onClick="mayuri3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chicken"
        android:layout_marginTop="84dp"
        android:layout_below="@+id/juicy"
        android:layout_alignLeft="@+id/juicy"
        android:layout_alignStart="@+id/juicy" />
</RelativeLayout>
